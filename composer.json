{
    "name": "dotkernel/dot-validator",
    "description": "DotKernel validator component extending and customizing laminas-validator",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "keywords": [
        "validator",
        "services",
        "factories",
        "container",
        "zf2",
        "zf3",
        "service-manager"
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-validator": "^2.10",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.5",
        "dotkernel/dot-mapper": "^1.0",
        "dotkernel/dot-hydrator": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Validator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Validator\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
}
